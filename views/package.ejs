	unicode-range: U+0000-00FF, U+0131, U+0152-0153, U+02BB-02BC, U+02C6, U+02DA, U+02DC, U+2000-206F, U+2074, U+20AC, U+2122, U+2191, U+2193, U+2212, U+2215, U+FEFF, U+FFFD;
  }

#install {
    font-family: "Montserrat",sans-serif;
	width: 100%;
	height: 100%;
	position: fixed;
	top: 0px;
	left: 0px;
	background-color: rgba(0, 0, 0, .6);
	display: flex;
	align-items: center;
	justify-content: center;
	transition: .2s ease;
	-webkit-transition: .2s ease;
	-moz-transition: .2s ease;
	-ms-transition: .2s ease;
	-o-transition: .2s ease;
}

#install div {
	padding: 20px;
	box-sizing: border-box;
	width: 350px;
	background-color: #fff;
	box-shadow: 0px 5px 10px rgba(0, 0, 0, .4);
	transition: .2s ease;
	-webkit-transition: .2s ease;
	-moz-transition: .2s ease;
	-ms-transition: .2s ease;
	-o-transition: .2s ease;
}

#install div.hidden {
	margin-top: -300px;
}

#install div svg {
	height: 20px;
	margin-top: 7px;
	margin-left: 5px;
	float: left;
}

#install div a.button {
	padding: 2px 10px;
	box-sizing: border-box;
	color: #006951;
	display: block;
	float: right;
	border: 3px solid var(--third);
	cursor: pointer;
	transition: .1s ease;
	-webkit-transition: .1s ease;
	-moz-transition: .1s ease;
	-ms-transition: .1s ease;
	-o-transition: .1s ease;
}

#install p .space {
	width: 10px;
	display: inline-block;
	height: 5px;
}

#install .button:hover {
	border: 3px solid var(--primary);
}

#install.error .button:hover, #install.error .button {
	border: 3px solid #8f8f8f;
}

#install.error div {
	border: 5px solid #FF4136;
}

#install.error p {
	font-style: italic;
	font-weight: 600;
	color: rgb(231, 51, 41);
}

#install.error a.button {
	cursor: default;
	background-color: #c6c6c6;
	color: #444;
	border-color: #8f8f8f;
	opacity: .5;
}

#install.error svg {
	cursor: default;
	fill: rgb(143, 143, 143);
	opacity: .5;
}

#install h4 {
	font-size: 30px;
	margin-top: 50px;
	margin-bottom: 16px;
}

#install a.github {
	text-decoration: none;
	color: #333;
	display: block;
	margin-top: 16px;
}

#install a.docs {
	font-family: "Lexend Deca", sans-serif;
	text-decoration: none;
	color: var(--primary-text);
	border-bottom: 2px solid transparent;
	display: inline-block;
}

#install a.docs:hover {
	border-bottom: 2px solid var(--primary);
	color: var(--primary);
}

#install a.github:hover {
	text-decoration: underline;
}

#install a.github::before {
	content: "> ";
}

#install div a.button.remove {
	background-color: #ff6860;
	border-color: #ff6860;
	color: #66130f;
}

#install div a.button.remove:hover {
	border-color: #d13931;
}
</style>

<div id="install" class="error">
