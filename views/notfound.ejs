#install {
    font-family: "Montserrat",sans-serif;
	width: 100%;
	height: 100%;
	position: fixed;
	top: 0px;
	left: 0px;
	background-color: rgba(0, 0, 0, .6);
	display: flex;
	align-items: center;
	justify-content: center;
	transition: .2s ease;
	-webkit-transition: .2s ease;
	-moz-transition: .2s ease;
	-ms-transition: .2s ease;
	-o-transition: .2s ease;
}

#install div {
	min-width: 350px;
	padding: 20px 40px;
	box-sizing: border-box;
	background-color: #fff;
	box-shadow: 0px 5px 10px rgba(0, 0, 0, .4);
	border: 5px solid #FF4136;
	transition: .2s ease;
	-webkit-transition: .2s ease;
	-moz-transition: .2s ease;
	-ms-transition: .2s ease;
	-o-transition: .2s ease;
}

#install div.hidden {
	margin-top: -300px;
}

#install div svg {
	height: 15px;
	margin-top: 7px;
	margin-left: -20px;
	float: left;
}

#install p .space {
	width: 10px;
	display: inline-block;
	height: 5px;
}

#install .button:hover {
	border: 3px solid var(--primary);
}

#install.error .button:hover, #install.error .button {
	border: 3px solid #8f8f8f;
}
#install p {
	font-style: italic;
	font-weight: 600;
	color: rgb(231, 51, 41);
}

#install.error svg {
	cursor: default;
	fill: #8f8f8f;
	opacity: .5;
}

#install h4 {
	font-size: 30px;
	margin-top: 50px;
	margin-bottom: 16px;
}

#install h4 span {
	color: #48bf91;
	margin: 0px 10px;
}

#install h4 .pkgs {
	font-weight: 600;
	margin-left: 10px;
	color: #111;
	opacity: .3;
}

#install a {
	text-decoration: none;
	color: #333;
	display: block;
	margin-top: 16px;
}

#install a:hover {
	text-decoration: underline;
}

#install a::before {
	content: "> ";
}
