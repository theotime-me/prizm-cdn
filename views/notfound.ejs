<div id="install" class="error">
		<div>
			<svg width="73" height="23" viewBox="0 0 73 23">
				<rect x="19" width="32" height="5"/>
				<rect x="61.5355" width="15.6464" height="5" transform="rotate(45 61.5355 0)"/>
				<rect x="58.0815" y="18.6452" width="15.6464" height="5" transform="rotate(-45 58.0815 18.6452)"/>
				<rect x="3.53553" width="15.6464" height="5" transform="rotate(45 3.53553 0)"/>
				<rect x="0.0814781" y="18.6452" width="15.6464" height="5" transform="rotate(-45 0.0814781 18.6452)"/>
				<rect x="19" y="17" width="32" height="5"/>
			</svg>

			<h4></h4>
			<p>Incorrect API usage.</p>
			<a onmousedown="return false;" href="https://prizm.netlify.com/?show=alias-error">change this</a>
		</div>
	</div>

	<script>
	
	let url=document.location.pathname.split("/"),
		p = document.querySelector("p"),
		newUrl="";

	console.log(url);
	
	url.shift();
	
	if(url[0] == ""){
		newUrl+="/<span>&lt;alias&gt;</span>/";
	} else if (!["$","_","p","z"].includes(url[0])) {
		let alias = url[1].toLowerCase();

		alias = alias.length > 15 ? alias.substring(0, 12)+"..." : alias;

		newUrl+="/<span>"+alias+"</span>/";
		p.innerHTML = "Invalid alias.";
	}

	if(url[1] != undefined){
		let pkgs=url[1];
		if(pkgs.includes("|")){
			let splited=pkgs.split("|");
			
			if(pkgs[pkgs.length-1]==""){
				splited.pop()
			}
			
			newUrl+= "<span class='pkgs'>"+splited.join("|")+"</span>";
			
		}
	}

	console.log("oki");
	
	document.querySelector("h4").innerHTML=newUrl;
</script>
